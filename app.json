{
  "expo": {
    "name": "booksource",
    "slug": "booksource",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "booksource",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine":"hermes",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["READ_EXTERNAL_STORAGE"], 
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
