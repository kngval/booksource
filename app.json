{
  "expo": {
    "name": "vivlion",
    "slug": "vivlion",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "vivlion",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "edgeToEdgeEnabled": false,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kngval.vivlion"
    },
    "android": {
      "edgeToEdgeEnabled": false,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.kngval.vivlion"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "130529e3-e1f4-47c8-bd64-3cb1b37c1293"
      }
    },
    "owner": "kngval"
  }
}
