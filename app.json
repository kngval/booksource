{
  "expo": {
    "name": "vivlion",
    "slug": "vivlion",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "vivlion",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kngval.vivlion"
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.kngval.vivlion"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f0d7bac2-7cdf-4601-aa82-70de41755d91"
      }
    }
  }
}
